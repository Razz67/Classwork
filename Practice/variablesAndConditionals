// Exercise 1

let letter = "A";

switch (letter) {
    case "A":
    case "E":
    case "I":
    case "O":
    case "U":
        console.log(letter + " is a vowel");
        break;
    default:
        console.log(Letter + " Is not a Vowel");
        break;
}
// Exercise 2 Terenayr Operator

let number = 3;

oddEven = number % 2 === 0 ? "It's even" : "It's odd";
console.log(oddEven);














